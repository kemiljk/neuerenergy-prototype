le>\n          <v-card-text>\n            Upload spreadsheets, PDFs or other documents that include information about your sites.\n          </v-card-text>\n          <v-file-input\n            v-model=\"files\"\n            accept=\".doc, .docx, .xls, .xlsx, .pdf\"\n            color=\"secondary\"\n            counter\n            label=\"File input\"\n            multiple\n            placeholder=\"Select your files\"\n            prepend-icon=\"mdi-paperclip\"\n            outlined\n            :show-size=\"1000\"\n            hover\n            clearable\n            class=\"mt-4 mr-4 ml-2\"\n          >\n            <template v-slot:selection=\"{ index, text }\">\n              <v-chip\n                v-if=\"index < 2\"\n                color=\"secondary\"\n                dark\n                label\n                small\n              >\n                {{ text }}\n              </v-chip>\n\n              <span\n                v-else-if=\"index === 2\"\n                class=\"overline grey--text text--darken-3 mx-2\"\n              >\n                +{{ files.length - 2 }} File(s)\n              </span>\n            </template>\n          </v-file-input>\n          <div class=\"text-center\">\n            <v-btn class=\"secondary ml-4 mb-4\">\n              Upload\n            </v-btn>\n          </div>\n        </v-card>\n      </v-col>\n      <v-col cols=\"6\">\n        <v-card>\n           <v-card-title class=\"secondary--text font-weight-bold\">\n            Input data manually\n          </v-card-title>\n          <v-card-text>\n            Initiate site setup by adding the site title, location, total carbon emissions, total energy consumption and carbon footprint (if known).\n          </v-card-text>\n            <form class=\"mr-4 ml-4\">\n              <v-text-field\n                v-model=\"name\"\n                :error-messages=\"nameErrors\"\n                label=\"Site title\"\n                required\n                prepend-icon=\"mdi-subtitles\"\n                @input=\"$v.name.$touch()\"\n                @blur=\"$v.name.$touch()\"\n                color=\"secondary\"\n              ></v-text-field>\n              <v-text-field\n                v-model=\"name\"\n                :error-messages=\"nameErrors\"\n                label=\"Site location\"\n                required\n                prepend-icon=\"mdi-map-marker\"\n                @input=\"$v.name.$touch()\"\n                @blur=\"$v.name.$touch()\"\n                color=\"secondary\"\n              ></v-text-field>\n              <v-text-field\n                v-model=\"name\"\n                :error-messages=\"nameErrors\"\n                label=\"Total carbon emissions (per annum)\"\n                required\n                prepend-icon=\"mdi-weather-windy\"\n                @input=\"$v.name.$touch()\"\n                @blur=\"$v.name.$touch()\"\n                color=\"secondary\"\n              ></v-text-field>\n              <v-text-field\n                v-model=\"name\"\n                :error-messages=\"nameErrors\"\n                label=\"Total energy consumption (per annum)\"\n                required\n                prepend-icon=\"mdi-lightbulb-on\"\n                @input=\"$v.name.$touch()\"\n                @blur=\"$v.name.$touch()\"\n                color=\"secondary\"\n              ></v-text-field>\n              <v-text-field\n                v-model=\"name\"\n                :error-messages=\"nameErrors\"\n                label=\"Total carbon footprint (per annum, if known)\"\n                required\n                prepend-icon=\"mdi-cloud\"\n                @input=\"$v.name.$touch()\"\n                @blur=\"$v.name.$touch()\"\n                color=\"secondary\"\n              ></v-text-field>\n              <!-- <v-select\n                v-model=\"select\"\n                :items=\"items\"\n                :error-messages=\"selectErrors\"\n                label=\"Item\"\n                required\n                @change=\"$v.select.$touch()\"\n                @blur=\"$v.select.$touch()\"\n                ></v-select>\n                <v-checkbox\n                v-model=\"checkbox\"\n                :error-messages=\"checkboxErrors\"\n                label=\"Do you agree?\"\n                required\n                @change=\"$v.checkbox.$touch()\"\n                @blur=\"$v.checkbox.$touch()\"\n              ></v-checkbox> -->\n              <div class=\"text-center\">\n                <v-btn class=\"secondary mr-4 mb-4\" @click=\"submit\">submit</v-btn>\n                <v-btn class=\"secondary mb-4\" @click=\"clear\">clear</v-btn>\n              </div>\n            </form>\n          </v-card>\n        </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nexport default {\n  name: \"Sites\",\n    data: () => ({\n      files: [],\n    }),\n}\n</script>"],"sourceRoot":"src/views"}]}